{
  "name": "regexp-events",
  "version": "0.1.0",
  "description": "A simple RegExp event emitter",
  "main": "dist/regexp-events.min.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm-run-all --parallel build:**:*",
    "build:src": "rollup -c rollup.config.js",
    "build:min": "rollup -c rollup.min.config.js",
    "build:es": "rollup -c rollup.es.config.js",
    "build:es:min": "rollup -c rollup.es.min.config.js",
    "build:umd": "rollup -c rollup.umd.config.js",
    "test": "echo no tests yet"
