    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "uglify-js": "github:mishoo/UglifyJS2#harmony"
  },
  "dependencies": {
    "lodash.isfunction": "^3.0.8",
    "lodash.isnil": "^4.0.0",
    "lodash.isstring": "^4.0.1"
